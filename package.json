{
  "name": "nextcrazy-cli",
  "version": "0.2.1-3",
  "description": "A NextJS cli meant to be used with nextcrazy boilerplate",
  "main": "./lib/index.js",
  "bin": {
    "nextcrazy-cli": "bin/nextcrazy"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run build && npm run global",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha src/**/*.spec.ts",
    "build": "tsc -p . && cp src/templates lib",
    "global": "npm i -g && nextcrazy-cli",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src"
  },
  "files": [
    "bin/",
    "src/",
    "templates/"
  ],
  "author": {
    "email": "it.moh.ou@gmail.com",
    "name": "Mohammad AlOulabi",
    "url": "https://www.npmjs.com/~mohammadou1"
  },
  "license": "MIT",
  "dependencies": {
    "camelcase": "^6.0.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "inversify": "^5.0.1",
    "prompts": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.1",
    "write": "^2.0.0"
  },
  "repository": {
    "url": "https://github.com/mohammadou1/nextcrazy-cli"
  },
  "keywords": [
    "nextcrazy",
    "nextcrazy-cli",
    "cli",
    "nextjs"
  ],
  "devDependencies": {
    "@liftr/tscov": "^2.0.0",
    "@types/camelcase": "^5.2.0",
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^14.11.1",
    "@types/prompts": "^2.0.9",
    "@types/write": "^2.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.3"
  }
}
